{
    "name": "ludovicose/generator",
    "description": "Code generator for Laravel",
    "keywords": ["laravel", "generator", "code", "scaffold"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aibek Muratbekov",
            "email": "aibek.muratbekov@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Ludovicose\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ludovicose\\Generator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ludovicose\\Generator\\Providers\\CodeGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
